<%= form_for @user do |f| %>

  <p>
    <%= f.label "Email" %><br>
    <%= f.text_field "email" %>
  </p>
   <p>
    <%= f.label "First Name" %><br>
    <%= f.text_field "first_name" %>
  </p>
   <p>
    <%= f.label "Last Name" %><br>
    <%= f.text_field "last_name" %>
  </p>
   <p>
    <%= f.label "Password" %><br>
    <%= f.text_field "password" %>
  </p>
   <p>
    <%= f.submit "Create Account" %>
  </p>

<%end%>